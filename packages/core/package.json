    "logging",
    "logger",
    "node",
    "react-native",
    "browser",
    "debug",
    "json"
  ],
  "homepage": "https://github.com/koengommers/etholog#readme",
  "author": "Koen Gommers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koengommers/etholog.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
